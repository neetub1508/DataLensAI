# Snowflake Connection Configuration
# Copy this file to .env and replace with your actual Snowflake connection details

# Snowflake Account Information
SNOWFLAKE_ACCOUNT=your-account-name.region.aws
SNOWFLAKE_USER=your_username
SNOWFLAKE_PASSWORD=your_password
SNOWFLAKE_DATABASE=your_database
SNOWFLAKE_SCHEMA=your_schema
SNOWFLAKE_WAREHOUSE=your_warehouse
SNOWFLAKE_ROLE=ACCOUNTADMIN

# Snowflake JDBC Connection Parameters
SNOWFLAKE_URL=jdbc:snowflake://${SNOWFLAKE_ACCOUNT}.snowflakecomputing.com
SNOWFLAKE_DRIVER=net.snowflake.client.jdbc.SnowflakeDriver

# Optional Snowflake Connection Properties
SNOWFLAKE_CONNECTION_TIMEOUT=60000
SNOWFLAKE_NETWORK_TIMEOUT=300000
SNOWFLAKE_QUERY_TIMEOUT=300000
SNOWFLAKE_LOGIN_TIMEOUT=60000

# SSL Configuration (recommended for production)
SNOWFLAKE_SSL=on
SNOWFLAKE_VALIDATE_DEFAULT_PARAMETERS=true

# Connection Pool Settings
SNOWFLAKE_MAX_POOL_SIZE=10
SNOWFLAKE_MIN_POOL_SIZE=2
SNOWFLAKE_INITIAL_POOL_SIZE=2

# Application Name for Snowflake session tracking
SNOWFLAKE_APPLICATION=DataLensAI